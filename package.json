{
  "name": "searchmusic",
  "version": "1.0.0",
  "description": "Powerful search engine for musics, albums, artists and fit up them with related playlist and lyrics",
  "keywords": [
    "search music",
    "youtube music"
  ],
  "license": "MIT",
  "main": "./build/src/index.js",
  "types": "./build/src/index.d.ts",
  "files": [
    "/build"
  ],
  "author": "mr687 <davinomoehdanino@gmail.com>",
  "repository": "git@github.com:vookav2/searchmusic.git",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --no-notify --cls src/index.ts",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch --verbose",
    "build": "rimraf build && tsc",
    "lint": "eslint . --ext .ts",
    "pretty": "prettier --write .",
    "prepare": "husky install && yarn build"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.30",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "husky": "^7.0.4",
    "jest": "^27.4.1",
    "lint-staged": "^12.4.1",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.1",
    "ts-node-dev": "^1.1.8",
    "tsc-files": "^1.1.3",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "crypto-js": "^4.1.1",
    "got": "^11.8.2",
    "songlyrics": "^2.3.2"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "yarn eslint",
      "yarn prettier --write"
    ],
    "**/*.ts": [
      "tsc-files --noEmit"
    ]
  }
}
